	.arch msp430g2553
	.p2align 1,0
	.text			

	.extern note
	.extern count
	.extern scaleNote
	
	.extern buzzer_set_period
 	.extern clearScreen
	
	.global convert
convert:	
	
	MOV &scaleNote, R12		;move note to parameter
	CALL #assySong
	MOV R12, &note ; note = assySong(scaleNote)
	MOV &note, R12
	CALL #buzzer_set_period
	JMP end
end:
	ADD #2, R1		;free allocated space
	POP R0			;return


